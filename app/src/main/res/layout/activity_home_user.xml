<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".user.HomeUserActivity">

    <ImageView
        android:id="@+id/imageviewbooking"

        android:layout_width="180dp"
        android:layout_height="200dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="80dp"
        android:onClick="GoTOBooking"
        android:scaleType="centerCrop"
        android:src="@drawable/booking"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Booked Now"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageviewbooking"
        app:layout_constraintEnd_toEndOf="@+id/imageviewbooking"
        app:layout_constraintStart_toStartOf="@+id/imageviewbooking" />

    <ImageView
        android:id="@+id/imageviewFlaging"
        android:layout_width="180dp"
        android:layout_height="200dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="12dp"
        android:onClick="GoTOFlaging"
        android:scaleType="centerCrop"
        android:src="@drawable/flagingbg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageviewbooking"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Flaging and mapping"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageviewFlaging"
        app:layout_constraintEnd_toEndOf="@+id/imageviewFlaging"
        app:layout_constraintStart_toStartOf="@+id/imageviewFlaging" />

    <ImageView
        android:id="@+id/imageviewLocking"
        android:layout_width="190dp"
        android:layout_height="200dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="12dp"
        android:onClick="GoToLocking"
        android:paddingTop="12dp"
        android:paddingLeft="10dp"
        android:scaleType="centerCrop"
        android:src="@drawable/lockingaaa"
        app:layout_constraintEnd_toEndOf="@+id/imageviewbooking"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageviewbooking" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Locking  Unlocking"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageviewLocking"
        app:layout_constraintEnd_toEndOf="@+id/imageviewLocking"
        app:layout_constraintStart_toStartOf="@+id/imageviewLocking" />


    <ImageView
        android:id="@+id/imageviewChat"
        android:layout_width="180dp"
        android:layout_height="200dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="12dp"
        android:paddingLeft="12dp"
        android:background="@color/white"
        android:contentDescription="hiii"
        android:onClick="GoToChat"
        android:scaleType="fitCenter"
        android:src="@drawable/chataa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageviewLocking"
        app:layout_constraintTop_toBottomOf="@+id/imageviewFlaging" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chat Box"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageviewChat"
        app:layout_constraintEnd_toEndOf="@+id/imageviewChat"
        app:layout_constraintStart_toStartOf="@+id/imageviewChat" />

    <ImageView
        android:id="@+id/imageviewhelp"
        android:layout_width="180dp"
        android:layout_height="200dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:onClick="GoToHelp"
        android:scaleType="centerCrop"
        android:src="@drawable/help"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageviewLocking" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Help Desk"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageviewhelp"
        app:layout_constraintEnd_toEndOf="@+id/imageviewhelp"
        app:layout_constraintStart_toStartOf="@+id/imageviewhelp" />

    <ImageView
        android:id="@+id/imageviewAbout"
        android:layout_width="180dp"
        android:layout_height="200dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="12dp"
        android:onClick="GoTOAbout"
        android:scaleType="centerCrop"
        android:src="@drawable/about"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageviewhelp"
        app:layout_constraintTop_toBottomOf="@+id/imageviewChat" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageviewAbout"
        app:layout_constraintEnd_toEndOf="@+id/imageviewAbout"
        app:layout_constraintStart_toStartOf="@+id/imageviewAbout" />


</androidx.constraintlayout.widget.ConstraintLayout>